{
  "expo": {
    "name": "Rocket-Elevators-Mobile",
    "slug": "Rocket-Elevators-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "The Mobile application will allow teams in the field to interact with the Rocket Elevators information system, so it can only be accessed by employees. ",
    "githubUrl": "https://github.com/cris-s-santiago/Rocket-Elevators-Mobile.git"
  }
}
